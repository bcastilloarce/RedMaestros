{
  "version": 2,
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "framework": "nextjs",
  "env": {
    "DATABASE_URL": "@database_url",
    "KV_URL": "@kv_url",
    "KV_REST_API_TOKEN": "@kv_rest_api_token"
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "build": {
    "env": {
      "PRISMA_GENERATE": "true"
    }
  }
}
